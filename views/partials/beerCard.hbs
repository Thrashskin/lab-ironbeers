<div class="beer-card">
    <div> 
      <img class ="beer-card-img" src="{{this.image_url}}" alt="{{this.name}}">  
     </div>
  <div class="beer-content">
      <h3>{{this.name}}</h3>
        <p class="beer-text">
          Description: {{this.description}}
      </p>
      <p>
        Tagline: {{this.tagline}}
      </p>
   </div>
</div>